pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo "Building..."'
      }
    }

    stage('Test') {
      steps {
        sh 'echo "Testing.."'
      }
    }

    stage('Secure') {
      steps {
        sh 'echo "Securing..."'
      }
    }

    stage('Deploy') {
      steps {
        sh 'echo "Deploying..."'
      }
    }

  }
}